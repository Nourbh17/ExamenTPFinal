{% extends 'layout.html.twig' %}

{% block title %}ALL PFES!{% endblock %}

{% block body %}
    <div class="row">
        {% for pfe in pfes %}
            <div class="card text-white col-sm-4 m-2" style="width: 18rem;">

                <div class="card-body">
                    <h5 class="card-title">{{ pfe.Titre }}</h5>
                    <p class="card-text">Student: {{ pfe.student }}<br> Entreprise:{{ pfe.entreprise }}</p>
                </div>
            </div>
        {% endfor %}
    </div>
{% endblock %}